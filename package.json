{
  "name": "nvim-ui-modifier",
  "displayName": "nvim-ui-modifier",
  "description": "Change Theme or accent colors of UI by changing NVim modes",
  "version": "0.0.2",
  "publisher": "JuilanIaquinandi",
  "repository": {
    "url": "https://github.com/kensleDev/nvim-theme-modifier"
  },
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:nvim-theme.normal",
    "onCommand:nvim-theme.insert",
    "onCommand:nvim-theme.visual",
    "onCommand:nvim-theme.replace"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nvim-theme.normal",
        "title": "NVIM Normal"
      },
      {
        "command": "nvim-theme.insert",
        "title": "NVIM Insert"
      },
      {
        "command": "nvim-theme.visual",
        "title": "NVIM Visual"
      },
      {
        "command": "nvim-theme.replace",
        "title": "NVIM Replace"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js",
    "release": "npx vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.48.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}
