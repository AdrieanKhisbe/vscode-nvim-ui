{
  "name": "nvim-theme-modifier",
  "displayName": "nvim-theme-modifier",
  "description": "swap color of ui based on nvim mode ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:nvim-theme-switcher.normal",
    "onCommand:nvim-theme-switcher.insert",
    "onCommand:nvim-theme-switcher.visual",
    "onCommand:nvim-theme-switcher.replace"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nvim-theme-switcher.normal",
        "title": "NVIM Normal"
      },
      {
        "command": "nvim-theme-switcher.insert",
        "title": "NVIM Insert"
      },
      {
        "command": "nvim-theme-switcher.visual",
        "title": "NVIM Visual"
      },
      {
        "command": "nvim-theme-switcher.replace",
        "title": "NVIM Replace"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.48.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}
